<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="617.0" prefWidth="729.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.afpa.gui.FormController">
   <children>
      <HBox layoutX="15.0" layoutY="20.0" spacing="30.0">
         <children>
            <TableView prefHeight="576.0" prefWidth="448.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Nom" />
                <TableColumn prefWidth="75.0" text="Prénom" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <VBox prefHeight="569.0" prefWidth="207.0" spacing="30.0">
               <children>
                  <VBox>
                     <children>
                        <Label text="Nom :">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField prefHeight="36.0" prefWidth="207.0" promptText="Nom" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Prénom :">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField prefHeight="36.0" prefWidth="207.0" promptText="Prenom" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Ville :">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField prefHeight="36.0" prefWidth="207.0" promptText="Ville" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" spacing="30.0">
                     <children>
                        <Button fx:id="btnAjouter" mnemonicParsing="false" onAction="#addClient" text="Ajouter">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="btnSupprimer" mnemonicParsing="false" onAction="#deleteClient" text="Supprimer">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="btnModifier" mnemonicParsing="false" onAction="#updateClient" text="Modifier">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
